<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="239.0" prefWidth="547.0" spacing="20.0" style="-fx-background-color: #4267b2;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.javachatbot.Login">
   <children>
      <AnchorPane prefHeight="227.0" prefWidth="468.0">
         <children>
            <Label layoutX="42.0" layoutY="36.0" text="Username: " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="txtUser" layoutX="163.0" layoutY="30.0" prefHeight="25.0" prefWidth="277.0" promptText="Enter Username">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <Label layoutX="42.0" layoutY="79.0" text="Password: " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Button fx:id="btnCancel" layoutX="163.0" layoutY="152.0" mnemonicParsing="false" text="Cancel">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btnReg" layoutX="251.0" layoutY="152.0" mnemonicParsing="false" onAction="#on_btnReg_clicked" prefHeight="32.0" prefWidth="103.0" text="Register">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btnLogin" layoutX="363.0" layoutY="152.0" mnemonicParsing="false" onAction="#on_btnLogin_clicked" text="Login">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <PasswordField fx:id="txtPwd" layoutX="163.0" layoutY="77.0" prefHeight="32.0" prefWidth="277.0" promptText="Enter Password">
               <font>
                  <Font size="16.0" />
               </font>
            </PasswordField>
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
